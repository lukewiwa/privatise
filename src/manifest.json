{
  "name": "Privatise",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "An extension to open the current tab in a private tab.",
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "img/eye-slash-fill-16.png",
    "48": "img/eye-slash-fill-48.png",
    "128": "img/eye-slash-fill-128.png"
  }
}